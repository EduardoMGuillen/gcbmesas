{
  "name": "tablecontrol",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:network": "next dev -H 0.0.0.0",
    "dev:tunnel": "concurrently \"npm run dev\" \"lt --port 3000\"",
    "dev:ngrok": "concurrently \"npm run dev\" \"ngrok http 3000\"",
    "get-ip": "node scripts/get-ip.js",
    "build": "node scripts/setup-env.js && prisma generate && npm run db:setup && next build",
    "db:setup": "node scripts/db-setup-with-timeout.js",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:fix-password": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/fix-admin-password.ts",
    "db:verify-password": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/verify-admin-password.ts",
    "import:bebidas": "node scripts/import-bebidas-wrapper.js",
    "cap:sync": "cap sync",
    "cap:android": "cap sync android && cap open android",
    "android": "cap sync android && cap open android",
    "build:apk": "cap sync android && cd android && build-apk.bat"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/push-notifications": "^6.0.1",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "bcryptjs": "^2.4.3",
    "firebase": "^12.8.0",
    "firebase-admin": "^12.0.0",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.4",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "nodemailer": "^7.0.13",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "@paypal/react-paypal-js": "^8.7.0",
    "@vercel/blob": "^0.27.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.9",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
